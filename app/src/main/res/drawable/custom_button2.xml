<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="13dp" />
            <stroke android:width="3dp" android:color="#ff8c00" />
            <gradient android:angle="-90" android:startColor="#c68c19" android:endColor="#ffae00"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="13dp" />
            <stroke android:width="3dp" android:color="#c68c19" />
            <solid android:color="#ffae00"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="13dp" />
            <stroke android:width="3dp" android:color="#c68c19" />
            <gradient android:angle="-90" android:startColor="#ffae00" android:endColor="#ffae00" />
        </shape>
    </item>
</selector>